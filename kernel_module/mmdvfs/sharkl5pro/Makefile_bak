$(warning In sharkl5pro)
mm_dvfs-objs :=

DVFS_DIR:=$(shell find $(srctree)/$(src) -type d)
DVFS_INC := $(addprefix -I,$(DVFS_DIR))

ccflags-y += $(DVFS_INC)
ccflags-y += -I$(srctree)/drivers/devfreq/

CPP_DVFS_OBJS := cpp/cpp_dvfs_devfreq.o \
	cpp/cpp_dvfs_governor.o \
	driver/cpp_dvfs_sharkl5pro.o

DCAM_DVFS_OBJS := dcam/dcam_dvfs_devfreq.o \
	dcam/dcam_dvfs_governor.o \
	driver/dcam_dvfs_sharkl5pro.o

DCAMAXI_DVFS_OBJS := dcamaxi/dcamaxi_dvfs_devfreq.o \
	dcamaxi/dcamaxi_dvfs_governor.o \
	driver/dcamaxi_dvfs_sharkl5pro.o

JPG_DVFS_OBJS := jpg/jpg_dvfs_devfreq.o \
	jpg/jpg_dvfs_governor.o \
	driver/jpg_dvfs_sharkl5pro.o

FD_DVFS_OBJS := fd/fd_dvfs_devfreq.o \
	fd/fd_dvfs_governor.o \
	driver/fd_dvfs_sharkl5pro.o

ISP_DVFS_OBJS := isp/isp_dvfs_devfreq.o \
	isp/isp_dvfs_governor.o \
	driver/isp_dvfs_sharkl5pro.o

MTX_DVFS_OBJS := mtx/mtx_dvfs_devfreq.o \
	mtx/mtx_dvfs_governor.o \
	driver/mtx_dvfs_sharkl5pro.o

mm_dvfs-objs += mmsys/mmsys_dvfs_devfreq.o \
	mmsys/mmsys_dvfs_governor.o \
	mmsys_comm/mm_dvfs_queue.o \
	mmsys_comm/mmsys_dvfs_comm.o \
	driver/mmsys_dvfs_sharkl5pro.o

mm_dvfs-objs += $(CPP_DVFS_OBJS)
mm_dvfs-objs += $(DCAM_DVFS_OBJS)
mm_dvfs-objs += $(DCAMAXI_DVFS_OBJS)
mm_dvfs-objs += $(JPG_DVFS_OBJS)
mm_dvfs-objs += $(FD_DVFS_OBJS)
mm_dvfs-objs += $(ISP_DVFS_OBJS)
mm_dvfs-objs += $(MTX_DVFS_OBJS)

obj-y += mm_dvfs.o




